import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;

public class FilaSupermercado {

	public FilaSupermercado() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
  
		 
		 
        
        
        Map<Integer, Integer> cola = new HashMap<Integer, Integer>();
        Map<Integer, Integer> colaB = new HashMap<Integer, Integer>();
        		

        cola.put(5, 1);
        cola.put(10, 2);
        cola.put(12, 3);
        cola.put(1, 4);
        
        
        
        for (Entry<Integer, Integer> entry : cola.entrySet()) {
        	
        	
        	if(entry.getKey()>5) {
        		colaB.put(entry.getKey(),entry.getValue());
        		cola.remove(entry.getKey());
        	}
        	
            //System.out.println("clave=" + entry.getKey() + ", valor=" + entry.getValue());
        }
        

        
     
        System.out.println(cola);
        System.out.println(colaB);

    
	}

}
